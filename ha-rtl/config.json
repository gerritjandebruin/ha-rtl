{
    "name": "HA-RTL",
    "version": "1.1.2dev",
    "slug": "HA-RTL",
    "description": 
    "Use a RTL-SDR device to capture any RF signal and send it to Home Assistant. Before starting, complete configuration. After starting, a device_trigger is created which will update when the right RF pulse is received.",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "url": "http://github.com/gerritjandebruin/home-assistant-rtl-sdr",
    "usb": true,
    "startup": "application",
    "stage": "stable",
    "boot": "auto",
    "image": "gerritjandebruin/ha-rtl-{arch}",
    "services": [
      "mqtt:need"
    ],
    "options": {
      "frequency": "433920000",
      "discovery_prefix_mqtt": "homeassistant/device_automation",
      "trigger_type": "action",
      "trigger_subtype": "button_1",
      "manufacturer": "",
      "device_name": "rtl433",
      "id": "rtl433",
      "model": "rtl433"
    },
    "schema": {
      "frequency": "int(1,)",
      "protocol": "int?",
      "discovery_prefix_mqtt": "str",
      "trigger_type": "str",
      "trigger_subtype": "str",
      "manufacturer": "str",
      "device_name": "str",
      "id": "str",
      "model": "str"
    },
    "panel_icon": "mdi:radio-tower"
  }